{
  "compilerOptions": {
    "outDir": "lib",
    "moduleResolution": "node",
    "target": "es5",
    "module": "es2015",
    "skipDefaultLibCheck": true,
    "noUnusedLocals": true,
    "lib": ["es7", "dom"],
    "types": ["jest", "node"],
    "checkJs": false,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "allowJs": false,
    "strictNullChecks": true,
    "useDefineForClassFields": true,
    "baseUrl": "packages",
    "paths": {
      "--vital--": ["__vital__/src/"],
      "--vital--/*": ["__vital__/src/*"],
      "@asemirsk/accordion": ["bodiless-accordion/src"],
      "@asemirsk/accordion/*": ["bodiless-accordion/src/*"],
      "@asemirsk/bv": ["bodiless-bv/src"],
      "@asemirsk/bv/*": ["bodiless-bv/src/*"],
      "@asemirsk/card": ["bodiless-card/src"],
      "@asemirsk/card/*": ["bodiless-card/src/*"],
      "@asemirsk/carousel": ["bodiless-carousel/src"],
      "@asemirsk/components": ["bodiless-components/src"],
      "@asemirsk/components/*": ["bodiless-components/src/*"],
      "@asemirsk/components-ui": ["bodiless-components-ui/src"],
      "@asemirsk/components-ui/*": ["bodiless-components-ui/src/*"],
      "@asemirsk/core": ["bodiless-core/src"],
      "@asemirsk/core/*": ["bodiless-core/src/*"],
      "@asemirsk/core-ui": ["bodiless-core-ui/src"],
      "@asemirsk/core-ui/*": ["bodiless-core-ui/src/*"],
      "@asemirsk/curator/": ["bodiless-curator/src/"],
      "@asemirsk/curator/*": ["bodiless-curator/src/*"], 
      "@asemirsk/filtering": ["bodiless-filtering/src"],
      "@asemirsk/filtering/*": ["bodiless-filtering/src/*"],
      "@asemirsk/hydration": ["bodiless-hydration/src"],
      "@asemirsk/hydration/*": ["bodiless-hydration/src/*"],
      "@asemirsk/layouts": ["bodiless-layouts/src"],
      "@asemirsk/layouts/*": ["bodiless-layouts/src/*"],
      "@asemirsk/layouts-ui": ["bodiless-layouts-ui/src"],
      "@asemirsk/layouts-ui/*": ["bodiless-layouts-ui/src/*"],
      "@asemirsk/navigation": ["bodiless-navigation/src"],
      "@asemirsk/navigation/*": ["bodiless-navigation/src/*"],
      "@asemirsk/oidc": ["bodiless-oidc/src"],
      "@asemirsk/oidc/*": ["bodiless-oidc/src/*"],
      "@asemirsk/organisms": ["bodiless-organisms/src"],
      "@asemirsk/organisms/*": ["bodiless-organisms/src/*"],
      "@asemirsk/page": ["bodiless-page/src"],
      "@asemirsk/page/*": ["bodiless-page/src/*"],
      "@asemirsk/richtext": ["bodiless-richtext/src"],
      "@asemirsk/richtext/*": ["bodiless-richtext/src/*"],
      "@asemirsk/richtext-ui": ["bodiless-richtext-ui/src"],
      "@asemirsk/richtext-ui/*": ["bodiless-richtext-ui/src/*"],
      "@asemirsk/schema-org": ["bodiless-schema-org/src"],
      "@asemirsk/schema-org/*": ["bodiless-schema-org/src/*"],
      "@asemirsk/search": ["bodiless-search/src"],
      "@asemirsk/search/*": ["bodiless-search/src/*"],
      "@asemirsk/table": ["bodiless-table/src"],
      "@asemirsk/table/*": ["bodiless-table/src/*"],
      "@asemirsk/tokens": ["bodiless-tokens/src"],
      "@asemirsk/tokens/*": ["bodiless-tokens/src/*"],
      "@asemirsk/ui": ["bodiless-ui/src"],
      "@asemirsk/ui/*": ["bodiless-ui/src/*"],
      "@asemirsk/vital-buttons": ["vital-buttons/src/"],
      "@asemirsk/vital-buttons/*": ["vital-buttons/src/*"],      
      "@asemirsk/vital-editors": ["vital-editors/src/"],
      "@asemirsk/vital-editors/*": ["vital-editors/src/*"],
      "@asemirsk/vital-elements": ["vital-elements/src/"],
      "@asemirsk/vital-elements/*": ["vital-elements/src/*"],
      "@asemirsk/vital-flowcontainer": ["vital-flowcontainer/src/"],
      "@asemirsk/vital-flowcontainer/*": ["vital-flowcontainer/src/*"],
      "@asemirsk/vital-image": ["vital-image/src/"],
      "@asemirsk/vital-image/*": ["vital-image/src/*"],
      "@asemirsk/vital-layout": ["vital-layout/src/"],
      "@asemirsk/vital-layout/*": ["vital-layout/src/*"],
      "@asemirsk/vital-link": ["vital-link/src/"],
      "@asemirsk/vital-link/*": ["vital-link/src/*"],
      "@asemirsk/vital-list": ["vital-list/src/"],
      "@asemirsk/vital-list/*": ["vital-list/src/*"],
      "@asemirsk/vital-meta": ["vital-meta/src/"],
      "@asemirsk/vital-meta/*": ["vital-meta/src/*"],
      "@asemirsk/vital-navigation": ["vital-navigation/src/"],
      "@asemirsk/vital-navigation/*": ["vital-navigation/src/*"],
      "@asemirsk/vital-table": ["vital-table/src/"],
      "@asemirsk/vital-table/*": ["vital-table/src/*"],        
      "@asemirsk/vital-templates": ["vital-templates/src/"],
      "@asemirsk/vital-templates/*": ["vital-templates/src/*"],
      "@asemirsk/vital-youtube": ["vital-youtube/src"],
      "@asemirsk/vital-youtube/*": ["vital-youtube/src/*"],  
      "@asemirsk/vital-card": ["vital-card/src/"],
      "@asemirsk/vital-card/*": ["vital-card/src/*"],
      "@asemirsk/youtube": ["bodiless-youtube/src"],
      "@asemirsk/youtube/*": ["bodiless-youtube/src/*"],
      "@asemirsk/webpack": ["bodiless-webpack/src"],
      "@asemirsk/webpack/*": ["bodiless-webpack/src/*"],
      "@asemirsk/fclasses": ["fclasses/src"],
      "@asemirsk/fclasses/*": ["fclasses/src/*"],
      "@asemirsk/gatsby-theme-bodiless": ["gatsby-theme-bodiless/src"],
      "@asemirsk/gatsby-theme-bodiless/*": ["gatsby-theme-bodiless/src/*"]
    }
  }
}
