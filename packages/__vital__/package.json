{
  "name": "--vital--",
  "sideEffects": false,
  "version": "1.0.0-beta.18",
  "description": "Starter template for a Design System package based on Vital DS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "license": "Apache-2.0",
  "private": true,
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js",
    "/tailwind.config.js",
    "/doc",
    "./shadow.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:api-doc",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --out doc/api ./src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@asemirsk/cli": "^1.0.0-beta.18",
    "@asemirsk/components": "^1.0.0-beta.18",
    "@asemirsk/components-ui": "^1.0.0-beta.18",
    "@asemirsk/core": "^1.0.0-beta.18",
    "@asemirsk/fclasses": "^1.0.0-beta.18",
    "@asemirsk/richtext": "^1.0.0-beta.18",
    "@asemirsk/richtext-ui": "^1.0.0-beta.18",
    "@asemirsk/vital-editors": "^1.0.0-beta.18",
    "@asemirsk/vital-elements": "^1.0.0-beta.18",
    "@asemirsk/vital-flowcontainer": "^1.0.0-beta.18",
    "@asemirsk/vital-image": "^1.0.0-beta.18",
    "@asemirsk/vital-layout": "^1.0.0-beta.18",
    "@asemirsk/vital-link": "^1.0.0-beta.18",
    "@asemirsk/vital-list": "^1.0.0-beta.18",
    "@asemirsk/vital-table": "^1.0.0-beta.18",
    "@asemirsk/vital-templates": "^1.0.0-beta.18",
    "lodash": "^4.17.19",
    "tailwindcss": "^3.0.23"
  }
}
