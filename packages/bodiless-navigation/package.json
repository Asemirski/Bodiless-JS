{
  "name": "@asemirsk/navigation",
  "version": "2.0.1-beta.0",
  "description": "BodilessJs Navigation Components",
  "author": "Ivan Rudiuk <IRudiuk@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js",
    "/doc",
    "tailwind.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api src",
    "lint": "tslint ./src/**/*.ts?x",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,png}\" \"./lib/\""
  },
  "dependencies": {
    "@asemirsk/accordion": "^2.0.1-beta.0",
    "@asemirsk/card": "^2.0.1-beta.0",
    "@asemirsk/cli": "^1.0.0-beta.6",
    "@asemirsk/components": "^2.0.1-beta.0",
    "@asemirsk/components-ui": "^2.0.1-beta.0",
    "@asemirsk/core": "^2.0.1-beta.0",
    "@asemirsk/fclasses": "^2.0.1-beta.0",
    "@asemirsk/organisms": "^2.0.1-beta.0",
    "enzyme": "^3.9.0",
    "lodash": "^4.17.19",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "mobx": "^6.0.0",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  }
}
