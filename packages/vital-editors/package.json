{
  "name": "@asemirsk/vital-editors",
  "version": "1.1.0-beta.0",
  "description": "Editor components for the bodiless vital design system",
  "author": "Chris Odem <coden@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/",
    "/getDocs.js",
    "/tailwind.config.js",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p build:lib build:api-doc",
    "build:api-doc": "typedoc --out doc/api src",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@asemirsk/cli": "^1.1.0-beta.0",
    "@asemirsk/components": "^1.1.0-beta.0",
    "@asemirsk/components-ui": "^1.1.0-beta.0",
    "@asemirsk/core": "^1.1.0-beta.0",
    "@asemirsk/fclasses": "^1.1.0-beta.0",
    "@asemirsk/hydration": "^1.1.0-beta.0",
    "@asemirsk/layouts": "^1.1.0-beta.0",
    "@asemirsk/layouts-ui": "^1.1.0-beta.0",
    "@asemirsk/richtext": "^1.1.0-beta.0",
    "@asemirsk/richtext-ui": "^1.1.0-beta.0",
    "@asemirsk/tokens": "^1.1.0-beta.0",
    "@asemirsk/vital-elements": "^1.1.0-beta.0",
    "@asemirsk/vital-link": "^1.1.0-beta.0",
    "lodash": "^4.17.19"
  }
}
