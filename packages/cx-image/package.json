{
  "name": "@asemirsk/cx-image",
  "sideEffects": false,
  "version": "2.0.6-beta.0",
  "description": "Clean, designable Image components for CanvasX based projects",
  "author": "Marco Pagliarulo <mpaglia1@its.jnj.com>",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js",
    "/assets",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://sourcecode.jnj.com/scm/asx-nbfp/canvasx.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api ./src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@asemirsk/cli": "^1.0.0-beta.6",
    "@asemirsk/components": "^2.0.6-beta.0",
    "@asemirsk/components-ui": "^2.0.6-beta.0",
    "@asemirsk/core": "^2.0.6-beta.0",
    "@asemirsk/cx-elements": "^2.0.6-beta.0",
    "@asemirsk/fclasses": "^2.0.6-beta.0",
    "@asemirsk/gatsby-theme-bodiless": "^2.0.6-beta.0",
    "@asemirsk/hydration": "^2.0.6-beta.0",
    "@asemirsk/layouts": "^2.0.6-beta.0",
    "@asemirsk/layouts-ui": "^2.0.6-beta.0",
    "lodash": "^4.17.19"
  }
