  "private": true,
  "main": "lib/bodiless-example.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@bodiless/backend": "^0.0.35",
    "@bodiless/bv": "^0.0.35",
    "@bodiless/components": "^0.0.35",
    "@bodiless/core": "^0.0.35",
    "@bodiless/core-ui": "^0.0.35",
    "@bodiless/documentation": "^0.0.35",
    "@bodiless/fclasses": "^0.0.35",
    "@bodiless/gatsby-theme-bodiless": "^0.0.35",
    "@bodiless/gatsby-plugin-ssi": "^0.0.35",
    "@bodiless/layouts": "^0.0.35",
    "@bodiless/layouts-ui": "^0.0.35",
    "@bodiless/migration-tool": "^0.0.35",
    "@bodiless/organisms": "^0.0.35",
    "@bodiless/psh": "^0.0.35",
    "@bodiless/richtext": "^0.0.35",
    "@bodiless/richtext-ui": "^0.0.35",
    "@bodiless/ui": "^0.0.35",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.0.0",
    "gatsby": "^2.9.11",
    "gatsby-plugin-canonical-urls": "^2.1.18",
    "gatsby-plugin-sitemap": "^2.2.24",
    "lodash": "^4.17.11",
    "mobx-react-lite": "^1.3.1",
    "nuka-carousel": "^4.4.7",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^1.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:css build:lib build:scripts psh-validate",
    "build:watch": "run-p -r dev-frontend",
    "build:lib": "gatsby build",
    "build:css": "tailwind build ./src/components/index.tailwind.css -c ./tailwind.config.js -o ./src/components/index.css",
    "build:scripts": "tsc -p ./tsconfig.json",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf ./public/* ./.cache",
    "dev-frontend": "gatsby develop -p 8005",
    "dev-frontend-psh": "GATSBY_WEBPACK_PUBLICPATH=\"https://edit.${PLATFORM_ENVIRONMENT}-${PLATFORM_PROJECT}.${PLATFORM_ZONE}\" gatsby develop --port ${PORT:-8000}",
    "dev-frontend-docker": "gatsby develop --host=0.0.0.0",
    "start-docker": "run-p -r dev-frontend-docker",
    "serve": "gatsby serve",
    "docs:build": "bl-docs-build",
    "docs:serve": "docsify serve ./doc",
    "docs": "run-s docs:build docs:serve",
    "psh-update": "node ./lib/scripts/bodiless-psh-update",
    "psh-validate": "node ./lib/scripts/bodiless-psh-validate"
  },
  "devDependencies": {
    "babel-plugin-tailwind-components": "^0.5.10",
    "babel-preset-gatsby": "^0.1.11",
    "copyfiles": "^2.1.1",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.2.4"
  }
}
