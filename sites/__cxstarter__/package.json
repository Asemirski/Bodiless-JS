{
  "name": "@sites/__cxstarter__",
  "version": "2.0.1-beta.0",
  "description": "Starter site for a canvasx-based Bodiless site",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "private": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "dependencies": {
    "@asemirsk/__cxstarter__": "^2.0.1-beta.0",
    "@asemirsk/backend": "^1.0.0-beta.6",
    "@asemirsk/cli": "^1.0.0-beta.6",
    "@asemirsk/components": "^2.0.1-beta.0",
    "@asemirsk/core": "^2.0.1-beta.0",
    "@asemirsk/cx-editors": "^2.0.1-beta.0",
    "@asemirsk/cx-elements": "^2.0.1-beta.0",
    "@asemirsk/cx-link": "^2.0.1-beta.0",
    "@asemirsk/cx-templates": "^2.0.1-beta.0",
    "@asemirsk/documentation": "^1.0.0-beta.6",
    "@asemirsk/fclasses": "^2.0.1-beta.0",
    "@asemirsk/gatsby-theme-bodiless": "^2.0.1-beta.0",
    "@asemirsk/search": "^2.0.1-beta.0",
    "@asemirsk/ui": "^2.0.1-beta.0",
    "@asemirsk/webpack": "^1.0.0-beta.6",
    "@babel/core": "^7.9.0",
    "autoprefixer": "^10.0.0",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "gatsby": "~3.13.0",
    "gatsby-plugin-canonical-urls": "^3.6.0",
    "gatsby-plugin-sitemap": "^4.2.0",
    "glob": "^7.1.6",
    "informed": "^3.34.0",
    "lodash": "^4.17.19",
    "mobx-react": "^7.2.1",
    "ora": "^4.0.2",
    "postcss": "^8.2.4",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.0",
    "react-mde": "^11.0.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:lib build:doc build:search",
    "build:doc": "bl-docs-build",
    "build:env-vars": "bodiless generate-env-vars",
    "build:lib": "bl-clear-logs && gatsby build && bl-validate-logs",
    "build:search": "npm run search-index",
    "build:stats": "BODILESS_BUILD_STATS=1 npm run build",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf ./public/* ./.cache",
    "dev-backend": "bodiless-backend",
    "dev-frontend": "gatsby develop -p 8005",
    "prepare": "node ../../scripts/set-missing-symlinks.js @bodiless",
    "prestart": "run-s build:env-vars build:doc",
    "docs:serve": "docsify serve ./doc",
    "docs": "run-s build:doc docs:serve",
    "search-index": "create-search-index",
    "serve": "gatsby serve",
    "start": "run-p -r dev-frontend dev-backend"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.26",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^2.0.2",
    "tailwindcss-dir": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.0.0"
  }
}
